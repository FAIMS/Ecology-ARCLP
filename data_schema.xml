<?xml version="1.0" encoding="UTF-8"?>
<dataSchema>
  <ArchaeologicalElement name="entity">
    <description></description>

    <!--
        Protip: $1 => Vocabname,
                $2 => Measure,
                $3 => Freetext and
                $4 => Certainty
    -->
    <!--Physical Characteristics Tab Group-->
    <property name="recordId" type="measure" isIdentifier="true">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="scribe" type="freetext">
      <description/>
      <formatString><![CDATA[$3]]></formatString>
    </property>
    <property name="processingPerson" type="freetext">
      <description/>
      <formatString><![CDATA[$3]]></formatString>
    </property>
    <property name="totalWeight" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="bagWeight" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="pes" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="tailWidth" type="freetext">
      <description/>
      <formatString><![CDATA[$3]]></formatString>
    </property>
    <property name="condition" type="vocab">
      <description/>
      <lookup>
        <term> {G}
          <description></description>
        </term>
        <term> {F}
          <description></description>
        </term>
        <term> {N_A}
          <description></description>
        </term>
      </lookup>
    </property>
    <property name="new" type="vocab">
      <description/>
      <lookup>
        <term> {Y}
          <description></description>
        </term>
        <term> {N}
          <description></description>
        </term>
        <term> {N_A}
          <description></description>
        </term>
      </lookup>
    </property>
    <property name="earTagNumber" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="dna" type="measure">
      <description/>
      <lookup>
        <term> {Y}
          <description></description>
        </term>
        <term> {N}
          <description></description>
        </term>
        <term> {N_A}
          <description></description>
        </term>
      </lookup>
    </property>
    <property name="collarFrequency" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="sex" type="freetext">
      <description/>
      <formatString><![CDATA[$3]]></formatString>
    </property>
    <property name="testes" type="freetext">
      <description/>
      <formatString><![CDATA[$3]]></formatString>
    </property>
    <property name="pouch" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="pySize" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>
    <property name="notes" type="measure">
      <description/>
      <formatString><![CDATA[$2]]></formatString>
    </property>





    <property name="prefix" type="measure" isIdentifier="true">
      <description></description>
      <formatString><![CDATA[$2]]></formatString>
    </property>

    <property name="note" type="measure">
      <description>This is a note</description>
      <formatString><![CDATA[$2]]></formatString>
    </property>

    <property name="choice" type="vocab">
      <description></description>
      <lookup>
        <term> {Yes}
          <description></description>
        </term>
        <term> {No}
          <description></description>
        </term>
      </lookup>
    </property>
  </ArchaeologicalElement>
</dataSchema>
